@model IEnumerable<WimbledonWines.Models.Order>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

 
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.OrderId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FullName)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.Address)
